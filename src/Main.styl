
body
  font-size 12px

.col-xs-15
  width 20%;
  float left;

@media (min-width: 768px)
  .col-sm-15
    width 20%
    float left

@media (min-width: 992px)
  .col-md-15
    width 20%
    float left

@media (min-width: 1200px)
  .col-lg-15
    width 20%
    float left


body
  color red
  letter-spacing 1.2px


.player
  border 1px solid yellow
  margin 5px
  height 360px
  border-radius 4px
  overflow hidden

.middle-sect
  margin 5px
  height 20px
  border 1px solid blue


.panel-heading
  padding 5px

.panel-group .panel+.panel
  margin-top 0px

.panel-group .panel
  border-radius 0px
  max-height 300px
  overflow hidden

.panel-body
  height 300px
  padding 0

.general-tab
  position relative
  width 100%
  height 100%
  background linear-gradient(rgba(0,0,0,.75), rgba(0,0,0,.75)),
    url(http://ddragon.leagueoflegends.com/cdn/img/champion/splash/Aatrox_0.jpg) no-repeat
  background-size cover
  background-position center

  color #fff
  font-weight 300
